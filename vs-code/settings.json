{

  "breadcrumbs.enabled": true,
  "css.lint.boxModel": "warning",
  "css.lint.duplicateProperties": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.important": "warning",
  "css.lint.universalSelector": "warning",
  "css.lint.zeroUnits": "warning",
  "editor.detectIndentation": false,
  "editor.fontSize": 14.5,
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 500,
  "editor.formatOnType": true,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "line",
  "editor.renderWhitespace": "boundary",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "elixirLS.dialyzerEnabled": false,
  "elixirLS.suggestSpecs": false,
  "files.autoSaveDelay": 200,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "html.format.endWithNewline": true,
  "scss.lint.boxModel": "warning",
  "scss.lint.compatibleVendorPrefixes": "warning",
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.float": "warning",
  "scss.lint.idSelector": "warning",
  "scss.lint.ieHack": "warning",
  "scss.lint.important": "warning",
  "terminal.integrated.fontSize": 14,
  "window.zoomLevel": 1,
  "workbench.editor.enablePreview": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tree.indent": 20,
  "files.associations": {
    "*.ex": "elixir",
    "*.eex": "html",
    "*.leex": "html"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "multiCommand.commands": [{
      "command": "multiCommand.splitRight",
      "sequence": [
        "workbench.action.splitEditor",
        "workbench.action.focusFirstEditorGroup",
        "workbench.action.closeActiveEditor"
      ]
    },
    {
      "command": "multiCommand.splitEditorDown",
      "sequence": [
        "workbench.action.splitEditorDown",
        "workbench.action.focusFirstEditorGroup",
        "workbench.action.closeActiveEditor"
      ]
    },
  ],
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#c5ffdd"
  },
  "workbench.iconTheme": "vscode-icons",
  "[jsonc]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "editor.showFoldingControls": "always",
  "workbench.colorTheme": "GitHub Sharp"
}
